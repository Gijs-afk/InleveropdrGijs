@model IEnumerable<Account>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table table-bordered" style="width: 100%">
    <thead>
        <tr>
            <th>
                Accounts Id
            </th>
            <th>
                Accounts klantNaam
            </th>
            <th>
                Accounts Email
            </th>
            <th>
                Accounts Punten
            </th>
            <th>
                Accounts AantalVrienden
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="10%">
                    @obj.Id
                </td>
                <td width="30%">
                    @obj.klantNaam
                </td>
                <td width="40%">
                    @obj.Email
                </td>
                <td width="10%">
                    @obj AantalVrienden
                </td>
                 <td width="10%">
                    @obj Punten
                </td>
            </tr>
        }
    </tbody>
</table>
